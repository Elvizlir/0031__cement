extends layout/main.pug


include ../blocks/s-main-page-catalog/_s-main-page-catalog.pug
include ../blocks/s-content/_s-content.pug
include ../blocks/s-contact/_s-contact.pug
include ../blocks/s-sidebar/_s-sidebar.pug
include ../blocks/s-catalog/_s-catalog.pug
include ../blocks/s-blog-page/_s-blog-page.pug
include ../blocks/s-page-banners/_s-page-banners.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'Страница новости'
	title= title
block pagecontent
	+mob-mnu()
	+header()
		+top-line()
		+top-nav()
	.container
		.page-row.row
			.col-lg-9
				+breadcrumb('Главная', 'Новости', title)
				+s-page-banners()
				+s-blog-page()
			.col-lg-3
				+s-sidebar()
	+footer()